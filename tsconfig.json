{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "baseUrl": "./",
      "paths": {
        "*": ["./src/*"]
      },
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": ["src", "modules"],
    "exclude": ["node_modules", "dist"]
}
